{
  "name": "color-tf",
  "version": "5.0.1",
  "description": "RGB, HSL, HSV, HWB and more color models convertor",
  "main": "proxy",
  "browser": "umd.js",
  "module": "src",
  "scripts": {
    "test": "babel-node --presets @babel/env test/index.spec",
    "rollup": "NODE_ENV=rollup npx rollup -c",
    "rollup:debug": "rm -rf dist && NODE_ENV=rollup rollup -c rollup.config.debug.js",
    "build": "rm -rf dist && npx babel --out-dir dist --source-type module src && npm run rollup && cp package.json dist/ && cp README.md dist/ && cp -r src dist/",
    "pub": "npm run build && npm publish dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caub/color-tf.git"
  },
  "keywords": [
    "color",
    "model",
    "convert",
    "RGB",
    "HSL",
    "HSV",
    "HWB"
  ],
  "author": "caub",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caub/color-tf/issues"
  },
  "homepage": "https://github.com/caub/color-tf#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/node": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^4.0.0-beta.4"
  },
  "dependencies": {}
}
